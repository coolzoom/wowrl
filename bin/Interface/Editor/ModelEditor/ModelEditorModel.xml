<Ui>
    <ModelFrame name="$parentModel" parent="ModelEditor" enableMouse="true" enableMouseWheel="true">
        <Anchors>
            <Anchor point="LEFT">
                <Offset>
                    <AbsDimension x="12"/>
                </Offset>
            </Anchor>
            <Anchor point="RIGHT" relativeTo="$parentSplitterThumbTexture" relativePoint="LEFT">
                <Offset>
                    <AbsDimension x="-5"/>
                </Offset>
            </Anchor>
            <Anchor point="TOP">
                <Offset>
                    <AbsDimension y="28"/>
                </Offset>
            </Anchor>
            <Anchor point="BOTTOM">
                <Offset>
                    <AbsDimension y="-12"/>
                </Offset>
            </Anchor>
        </Anchors>
        <Layers>
            <Layer>
                <FontString name="$parentFile" font="Fonts/Vera.ttf" fontHeight="10" justifyH="CENTER">
                    <Size>
                        <AbsDimension y="18"/>
                    </Size>
                    <Anchors>
                        <Anchor point="LEFT"/>
                        <Anchor point="RIGHT"/>
                        <Anchor point="BOTTOM">
                            <Offset>
                                <AbsDimension y="3"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad>
                this:SetLight(true, 0.5, 0.5, 0.5);
            </OnLoad>
        </Scripts>
    </ModelFrame>
</Ui>
