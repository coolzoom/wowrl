			<Add directory="..\include\xml" />
			<Add directory="..\include\utils" />
			<Add directory="..\dependencies\lua" />
		</Compiler>
		<Linker>
			<Add option="-Wl,--enable-runtime-pseudo-reloc" />
			<Add option="-Wl,--enable-auto-image-base" />
