            </Offset>
        </Anchor>
    </Anchors>
    
    <n[.,1]:TexCoords left="0.0" right="1.0" top="0.0" bottom="1.0"/>
    
    <n[.,1]:Color a="1.0" r="0.0" g="0.0" b="0.0"/>
    
    <n[.,1]:Gradient orientation="HORIZONTAL">
        <n[.,1]:MinColor a="1.0" r="0.0" g="0.0" b="0.0"/>
        <n[.,1]:MaxColor a="1.0" r="0.0" g="0.0" b="0.0"/>
    </Gradient>
    
</Texture>

<d:Frame name inherits="" virtual="false" setAllPoints="false" hidden="false" alpha="1.0" parent="" toplevel="false" movable="false" resizable="false" frameStrata="PARENT" enableMouse="false" enableKeyboard="false">

    <n[.,1]:Size>
        <n[*]:AbsDimension x y/>
        <n[*]:RelDimension x="1.0" y="1.0"/>
    </Size>
    
    <n[.,1]:Anchors>
        <Anchor point="TOPLEFT" relativeTo="" relativePoint="TOPLEFT">
            <n[.,1]:Offset>
                <n[*]:AbsDimension x="0" y="0"/>
                <n[*]:RelDimension x="0.0" y="0.0"/>
            </Offset>
        </Anchor>
    </Anchors>
    
    <n[.,1]:TitleRegion>
        <n[.,1]:Size>
