<-------------------->
<-- FILE STRUCTURE -->
<-------------------->

<mesh>

    <n[1]:submeshes>
        <submesh material b:usesharedvertices="false" b:use32bitindexes="false" operationtype="triangle_list">
            <n[1]:faces n:count>
                <face n:v1 n:v2 n:v3/>
            </faces>
            
            <n[1]:geometry n:vertexcount>
                <vertexbuffer b:positions="true" b:normals="true" n:texture_coord_dimensions_0="2" n:texture_coords="1">
                    <vertex>
                        <n[1]:position n:x n:y n:z/>
                        <n[1]:normal n:x n:y n:z/>
                        <n[1]:texcoord n:u n:v/>
                    </vertex>
                </vertexbuffer>
            </geometry>
        </submesh>
    </submeshes>

    <n[.,1]:submeshnames>
        <submeshname name n:index/>
    </submeshnames>

</mesh>

